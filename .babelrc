{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }],
    "react"
  ],
  "plugins": ["transform-class-properties", "syntax-dynamic-import"],
  "env": {
    "server": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "react"
      ],
      "plugins": ["transform-class-properties", "dynamic-import-node"]
    }
  }
}
